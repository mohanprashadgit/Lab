# PCA simple
import numpy as np, matplotlib.pyplot as plt
from sklearn.decomposition import PCA
X = np.array([[4,0,3,5],[5,1,4,4],[0,2,1,3],[3,0,2,5]])
p = PCA(2).fit_transform(X)
plt.scatter(p[:,0], p[:,1])
for i in range(len(p)): plt.text(p[i,0], p[i,1], f'U{i+1}')
plt.title("PCA - 2 Components"); plt.show()

#1.cosine similarity
from sklearn.metrics.pairwise import cosine_similarity
import numpy as np
ratings = np.array([                # Example: users' movie ratings (rows = users, columns = movies)
    [4, 0, 3, 5],  # User 1
    [5, 1, 4, 4],  # User 2
    [0, 0, 5, 3]   # User 3
])
similarity = cosine_similarity(ratings)
print("Cosine Similarity Between Users:")
print(similarity)

